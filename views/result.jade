extends layout

block content
    div(style='text-align:center')开奖号码： #{result}
    div(style='text-align:center')投注号码： #{bet}
    div(style='text-align:center')投注金额： #{money} 元
    div(style='text-align:center')投注金额递增倍数： #{multiply} 次
    div(style='text-align:center')期数： #{times} 期
    div(style='text-align:center')赔率： #{odds} 倍
    div(style='text-align:center')命中次数： #{count} 期
    div(style='text-align:center')本金： #{principal} 元